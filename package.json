{
  "name": "linter-emscripten",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Lint C-based files using emscripten",
  "repository": {
    "type": "git",
    "url": "https://github.com/k2b6s9j/linter-emscripten"
  },
  "license": "MIT",
  "engines": {
    "atom": ">1.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^2.0.5",
    "atom-package-deps": "^2.0.1",
    "clang-flags": "^0.2.2"
  },
  "devDependencies": {
    "eslint": "^1.6.0"
  },
  "packages-deps": [
    "linter"
  ],
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "atom": true
    }
  }
}
